      "source": [
        "def missingNum(l1):\n",
        "  n=len(l1)\n",
        "  m=n+1\n",
        "  s=0\n",
        "  sumtotal=m*(m+1)/2\n",
        "  for i in range(len(l1)):\n",
        "    s=s+l1[i]\n",
        "  missnum=sumtotal-s\n",
        "  \n",
        "  print(\"Missing number is:\",missnum)\n",
        "\n",
        "n1=[1,2,5,4,9,8,7,3,10]\n",
        "n=len(n1)\n",
        "\n",
        "missingNum(n1)"
      ]
